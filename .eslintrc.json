{
    "extends": [
        "plugin:vue/essential"
    ],
    "globals": {
        "Vue": true,
        "Cake": true,
        "App": true
    },
    "rules": {
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ],
        "comma-dangle": [
            "error",
            "always-multiline"
        ],
        "no-trailing-spaces": "error",
        "no-console": "warn",
        "quotes": [
            "error",
            "single"
        ]
    }
}
